	<queryString>
		<![CDATA[SELECT proyecto.medico.licencia,
	proyecto.medico.nombre,
	proyecto.especialidad.id,
	proyecto.especialidad.nombre
FROM proyecto.medico_has_especialidad
	INNER JOIN proyecto.especialidad ON 
