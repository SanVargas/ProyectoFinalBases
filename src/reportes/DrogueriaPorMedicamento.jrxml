	<queryString>
		<![CDATA[SELECT proyecto.drogueria.nombre,
	proyecto.drogueria.nit
FROM proyecto.drogueria_has_medicamento
	INNER JOIN proyecto.drogueria ON 
	 proyecto.drogueria_has_medicamento.`Drogueria_nit` = proyecto.drogueria.nit 
	INNER JOIN proyecto.medicamento ON 
	 proyecto.drogueria_has_medicamento.`Medicamento_registroSanitario` = proyecto.medicamento.`registroSanitario`
	 WHERE proyecto.medicamento.nombre = $P{medicamento}]]>
